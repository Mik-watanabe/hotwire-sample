<p style="color: green"><%= notice %></p>

<h1>Clients</h1>

<div id="clients">
  <% @clients.each do |client| %>
    <%= render client %>
    <p>
      <%= link_to "Show this client", client %>
    </p>
  <% end %>
</div>
<%# ページネーションを表示する %>
<%= paginate @clients %>
<%= link_to "New client", new_client_path %>
